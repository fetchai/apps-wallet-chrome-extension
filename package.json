{
  "name": "my-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "autoprefixer": "7.1.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-preset-react-app": "^9.1.1",
    "babel-runtime": "6.26.0",
    "bn.js": "^5.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dom-storage": "^2.1.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.11.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "3.x",
    "fetchai-ledger-api": "0.0.1-alpha.7",
    "file-loader": "4.2.0",
    "flush-promises": "^1.0.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "infinite-scroll": "^3.0.6",
    "jest": "^25.1.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^25.1.0",
    "jest-environment-jsdom-global": "^1.2.1",
    "jest-jasmine2": "^25.1.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.7.2",
    "qrcode-generator": "^1.4.4",
    "raf": "3.4.0",
    "react": "^16.13.0",
    "react-collapsible": "^2.7.0",
    "react-dev-utils": "^9.0.0-next.68",
    "react-dom": "^16.13.0",
    "react-expand-animated": "^1.0.2",
    "react-frame-component": "^4.0.0",
    "react-infinite-scroller": "^1.2.4",
    "react-test-renderer": "^16.13.0",
    "resolve": "1.6.0",
    "route-lite": "^0.3.0",
    "sinon-chrome": "^3.0.1",
    "style-loader": "0.19.0",
    "styled-components": "^5.0.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "0.6.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "update-snapshots": "jest --updateSnapshot",
    "test2": "jest src/test",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand src/test  --env=jsdom",
    "lint": "eslint ."
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "modulePathIgnorePatterns": [
      "other_imported_modules"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/tests/utils/helper.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jsdom",
    "verbose": true,
    "testURL": "http://localhost:/",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "mini-css-extract-plugin": "^0.8.0",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0"
  }
}
